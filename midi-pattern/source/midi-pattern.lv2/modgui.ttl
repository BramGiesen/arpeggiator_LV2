@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://bramgiesen.com/midi-pattern>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-arp.html> ;
        modgui:stylesheet <modgui/stylesheet-arp.css> ;
        modgui:screenshot <modgui/midi-pattern_ss.png> ;
        modgui:thumbnail <modgui/midi-pattern_tn.png> ;
        modgui:brand "BG" ;
        modgui:label "MIDI Pattern" ;
        modgui:model "boxy" ;
        modgui:panel "8-sliders" ;
        modgui:color "white" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "velocityNote1" ;
            lv2:name "1" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "velocityNote2" ;
            lv2:name "2" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "velocityNote3" ;
            lv2:name "3" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "velocityNote4" ;
            lv2:name "4" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "velocityNote5" ;
            lv2:name "5" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "velocityNote6" ;
            lv2:name "6" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "velocityNote7" ;
            lv2:name "7" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "velocityNote8" ;
            lv2:name "8" ;
        ] ;
    ] .
