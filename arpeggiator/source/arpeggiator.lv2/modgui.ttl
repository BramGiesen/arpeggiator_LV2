@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://bramgiesen.com/arpeggiator>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-arpeggiator.html> ;
        modgui:stylesheet <modgui/stylesheet-arpeggiator.css> ;
        modgui:screenshot <modgui/screenshot-arpeggiator.png> ;
        modgui:thumbnail <modgui/thumbnail-arpeggiator.png> ;
        modgui:brand "BG" ;
        modgui:label "Arpeggiator" ;
        modgui:model "boxy" ;
        modgui:panel "1-select-4-knobs" ;
        modgui:color "white" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "arpMode" ;
            lv2:name "ArpMode" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Bpm" ;
            lv2:name "Bpm" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "noteLength" ;
            lv2:name "length" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "octaveSpread" ;
            lv2:name "octave" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "velocity" ;
            lv2:name "velocity" ;
        ] ;
    ] .
